<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:layout_gravity="center"
    android:background="@color/white"
    >

    <TextView
        android:id="@+id/wait4"
        android:layout_width="86dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="119dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="660dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="top"
        android:text="@string/wait"
        android:textAppearance="@style/wait"
        tools:ignore="ObsoleteLayoutParam,TextSizeCheck" />

    <TextView
        android:id="@+id/smart4"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="188dp"
        android:layout_marginTop="-720dp"
        android:layout_marginEnd="121dp"
        android:layout_marginBottom="660dp"
        android:fontFamily="@font/reem_kufi"
        android:gravity="top"
        android:text="@string/smart"
        android:textAppearance="@style/smart"
        tools:ignore="ObsoleteLayoutParam,TextSizeCheck,TextContrastCheck" />

    <ImageView
        android:id="@+id/illustration"
        android:layout_width="252dp"
        android:layout_height="252dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="-650dp"
        android:layout_marginEnd="71dp"
        android:layout_marginBottom="350dp"
        android:contentDescription="@string/todo"
        android:src="@drawable/illustration"
        tools:ignore="ObsoleteLayoutParam" />

    <TextView
        android:id="@+id/login_your"
        android:layout_width="265dp"
        android:layout_height="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="74dp"
        android:layout_marginTop="-340dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="376dp"
        android:gravity="top"
        android:text="Login your admin account"
        android:textAppearance="@style/login_your_"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,TextSizeCheck" />

    <ImageView
        android:id="@+id/rectangle_usern"
        android:layout_width="265dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/rectangle_snum"
        android:layout_marginStart="70dp"
        android:layout_marginTop="-340dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="660dp"
        tools:ignore="ContentDescription,ObsoleteLayoutParam" />

    <EditText
        android:id="@+id/enter_user"
        android:layout_width="246dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="-694dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="660dp"
        android:gravity="top"
        android:text="Enter your username"
        android:textAppearance="@style/enter_user"
        tools:ignore="Autofill,HardcodedText,LabelFor,ObsoleteLayoutParam,TextFields,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/lbl_usern"
        android:layout_width="66dp"
        android:layout_height="14dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="-715dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="376dp"
        android:gravity="top"
        android:text="USERNAME"
        android:textAppearance="@style/label"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,TextSizeCheck" />

    <ImageView
        android:id="@+id/rectangle_pass"
        android:layout_width="265dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:background="@drawable/rectangle_snum"
        android:layout_marginStart="70dp"
        android:layout_marginTop="-310dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="660dp"
        tools:ignore="ContentDescription,ObsoleteLayoutParam" />

    <EditText
        android:id="@+id/enter_pass"
        android:layout_width="246dp"
        android:layout_height="41dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="79dp"
        android:layout_marginTop="-694dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="660dp"
        android:gravity="top"
        android:text="Enter your password"
        android:textAppearance="@style/enter_user"
        tools:ignore="Autofill,HardcodedText,LabelFor,ObsoleteLayoutParam,TextFields,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/lbl_pass"
        android:layout_width="66dp"
        android:layout_height="14dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="-715dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="376dp"
        android:gravity="top"
        android:text="PASSWORD"
        android:textAppearance="@style/label"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,TextSizeCheck" />

    <ImageButton
        android:id="@+id/btn_login"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="154dp"
        android:layout_marginTop="-310dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="105dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/btn_login"
        tools:ignore="ContentDescription,ObsoleteLayoutParam,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/dont_acc"
        android:layout_width="150dp"
        android:layout_height="18dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="130dp"
        android:layout_marginTop="-100dp"
        android:layout_marginEnd="102dp"
        android:layout_marginBottom="376dp"
        android:gravity="center_horizontal|top"
        android:text="Don't have an account?"
        android:textAppearance="@style/dont_acc"
        tools:ignore="HardcodedText,ObsoleteLayoutParam,TextSizeCheck" />

    <ImageButton
        android:id="@+id/btn_register"
        android:layout_width="109dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="154dp"
        android:layout_marginTop="-370dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="105dp"
        android:backgroundTint="@color/white"
        android:src="@drawable/btn_reg"
        tools:ignore="ContentDescription,ObsoleteLayoutParam,SpeakableTextPresentCheck,TouchTargetSizeCheck" />





</LinearLayout>